@{
}
@using Online_English_Quiz_Project.Models
<div>
    <h1>result</h1>
    <p>id: @ViewBag.id</p>
    @foreach(UserQuiz uq in ViewBag.result)
    {
        <p>@uq.DateTaken</p>
        <p>@uq.Username</p>
        <p>@uq.Score</p>
        <p>@ViewBag.quiz.QuizTitle</p>
    }
</div>